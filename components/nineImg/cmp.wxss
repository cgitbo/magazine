/* components/nineImg/cmp.wxss */
.container {
  display: flex;
  flex-wrap: wrap;
}

.img-box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 700rpx;
  height: 700rpx;
  overflow: hidden;
  position: relative;
}

.img-num {
  padding: 10rpx 20rpx;
  font-size: 24rpx;
  position: absolute;
  right: 80rpx;
  bottom: 100rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, .7);
}

.img {
  width: 31%;
  height: 31%;
  margin-bottom: 20rpx;
}

.main-title {
  font-size: 40rpx;
  letter-spacing: 2rpx;
  padding: 0 20rpx;
}